{
  "name": "dofus-team",
  "version": "1.0.1",
  "description": "A conveniant window manager for Dofus team playing.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "debug": "electron . 'debug'",
    "dist": "rm -rf dist/ && build -wl --x64"
  },
  "build": {
    "win": {
      "extraResources": [
        {
          "from": "./build/",
          "to": "./../",
          "filter": [
            "*.exe"
          ]
        }
      ]
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Dofus"
  ],
  "author": "Pascal Boutin <contact@pboutin.net>",
  "license": "MIT",
  "devDependencies": {
    "electron": "~1.6.2",
    "electron-builder": "^18.1.0"
  },
  "dependencies": {
    "electron-settings": "^3.0.14"
  }
}
