{
  "name": "dofus-team",
  "version": "1.0.0",
  "description": "Multi-window switcher for Dofus built with Electron",
  "main": "./dist/main.js",
  "scripts": {
    "dev:electron": "tsc -w -p ./src/electron/tsconfig.json",
    "dev:ui": "parcel ./src/ui/styles.css ./src/ui/settings/settings.tsx",
    "start": "electron .",
    "debug": "electron . debug",
    "build": "rm -rf dist/ && electron-builder --x64 --windows portable"
  },
